{

  "manifest_version": 2,
  "name": "Garfield",
  "version": "1.0",

  "description": "Teh garfield extension",

  "icons": {
    "48": "icons/icon-48.png"
  },

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "icons/icon-48.png",
    "default_title": "Garfield!"
  },

  "background": {
    "scripts": ["garfield.js"]
  }

}